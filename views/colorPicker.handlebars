
<form action='/favorite_color' method='POST'>
    
        <p>What is your favorite color?</p>
        {{#each colors}}
        <p>
            <input id= 'color' type="radio" name="color" value="{{this}}"/>{{this}}
        </p>
        {{/each}}
        <input id='submit' type="submit" value="Submit"/>
</form>